#!/usr/bin/env ruby
# -*- coding: utf-8 -*-

require 'r-fxxk'

class ThinkingFace < Brainfuck
  nxt "洟避洟避洟能n"           # >
  prv "洟避洟避洟避洟能n"         # <
  inc "洟能n"               # +
  dec "洟避洟能n"             # -
  put "洟避洟避洟避洟避洟能n"       # .
  get "洟避洟避洟避洟避洟避洟能n"     # ,
  opn "洟避洟避洟避洟避洟避洟避洟能n"   # [
  cls "洟避洟避洟避洟避洟避洟避洟避洟能n" # ]
end

puts ThinkingFace.new.fuck(ARGF.read).force_encoding("UTF-8")
